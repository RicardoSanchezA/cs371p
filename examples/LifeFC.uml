@startuml
Life         *-  FredkinCell
AbstractCell ^-- ConwayCell
AbstractCell ^-- FredkinCell

class Life<T> {
    - vector<vector<T>> _grid
    + read ()
    + eval ()
    + print ()
    }

abstract class AbstractCell {
    - bool _state
    + {abstract} AbstractCell* clone () const
    + {abstract} evolve ()
    }

class ConwayCell {
    + ConwayCell* clone () const
    + evolve ()
    }

class FredkinCell {
    - int _age
    + FredkinCell* clone () const
    + evolve ()
    }

@enduml
